{
  "datasets": [
    {
      "name": "dataset_1",
      "type": "CHART",
      "url": "https://datahub.ki.agh.edu.pl/api/endpoints/71/data/",
      "static_data": [
        {
          "name": "Location",
          "source": "location.locName",
          "unit": null
