            },
            {
              "data_name": "Humidity",
              "plot_color": "yellow"
            }
          ]
        }
      ],

      "timestamp": {
        "from": "2022-05-15T15:33:16+02:00",
        "to": "now"
      },

      "filters": [
        {
          "@type": "OutlierFilter",
          "data_names": [
            "Temperature"
          ]
        },
        {
          "@type": "SortFilter",
          "data_name": "Temperature"
        }
      ],

      "updates": {
        "update": true,
        "update_interval_sec": 600
      }
    },
